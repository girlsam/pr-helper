{
  "name": "Pull Request Description Helper",
  "version": "0.1",
  "description": "Stages your PR with some descriptive markdown.",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*", 
        "http://github.com/*"
      ],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html"
  }
}